  "license": "ISC",
  "devDependencies": {
    "angular": "^1.6.1",
    "css-loader": "^0.26.1",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^1.16.3"
  }
}
