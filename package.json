{
  "name": "giphm",
  "version": "0.1.0",
  "description": "Just a gif carousel",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm-run-all --parallel dev:*",
    "dev:bundle": "webpack",
    "dev:sass-dev": "node-sass --watch app/styles/scss --output-style compressed --source-map true  app/styles/scss --output assets/css",
    "dev:serve": "webpack-dev-server --progress --inline --output-public-path=/assets/js",
